<h1>Your Songs</h1>

  <%@songs.each do |song|%>
    <h3><%= song.title %></h3>
    <audio controls>
      <source src=<%=song.stream_url+'?client_id='+ENV["CLIENT_ID"] %> type="audio/ogg">
    </audio>
    <%= button_to 'Delete', user_song_path(song.user_id,song.id), method: :delete %>
  <%end%>




